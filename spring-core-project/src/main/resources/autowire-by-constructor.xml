<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="payment" class="in.co.rays.autowire.byconstructor.Payment">
		<property name="balence" value="500"></property>
	</bean>

	<bean id="inventory" class="in.co.rays.autowire.byconstructor.Inventory">
		<property name="stock" value="10"></property>
	</bean>

	<bean id="order" class="in.co.rays.autowire.byconstructor.Order1"
		autowire="constructor">
	</bean>

</beans>